altura = float(input(""))
comprimento = float(input(""))
largura = float(input(""))
area_piso_sala = largura * comprimento
volume_sala = largura * comprimento * altura
area_parede = 2 * altura * largura + 2 * altura * comprimento
print(f'{area_piso_sala}')
print(f'{volume_sala}')
print(f'{area_parede}')
